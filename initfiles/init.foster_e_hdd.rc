import init.tegra.rc
import init.t210.rc
import init.foster_e_common.rc

on fs
    write /sys/block/sda/queue/scheduler cfq
    write /sys/block/sda/queue/iosched/read_expire 1000
    write /sys/block/sda/queue/iosched/write_expire 40000
    write /sys/block/sda/queue/iosched/writes_starved 16
    mount_all /fstab.foster_e_hdd
    swapon_all /fstab.foster_e_hdd

# factory reset protection
    mkdir /persistent 0660 system system
    setprop ro.frp.pst /dev/block/platform/tegra-sata.0/by-name/RP2


on post-fs-data
    mount ext4 /dev/block/platform/tegra-sata.0/by-name/RP3 /factory/wifi_config rw nosuid noexec nodev
    restorecon_recursive /factory/wifi_config
    mount ext4 /dev/block/platform/tegra-sata.0/by-name/RP3 /factory/wifi_config rw nosuid noexec nodev remount

on property:factory.wifi_country_code=*
    mount ext4 /dev/block/platform/tegra-sata.0/by-name/RP3 /factory/wifi_config rw remount
    chown system system /factory/wifi_config
    chown system system /factory/wifi_config/FSS.txt
    chown system system /factory/wifi_config/country_code.txt
    chown system system /factory/wifi_config/country_code.sig
    restorecon_recursive /factory/wifi_config
    start factoryconfig

on property:factory.sku_config=done
    mount ext4 /dev/block/platform/tegra-sata.0/by-name/RP3 /factory/wifi_config rw nosuid noexec nodev remount

service factoryconfig /system/bin/factoryconfig.sh
    class main
    user system
    group system sdcard_rw
    oneshot
